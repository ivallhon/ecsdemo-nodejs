{
    "executionRoleArn": "arn:aws:iam::302156821030:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "portMappings": [
          {
            "hostPort": 3000,
            "protocol": "tcp",
            "containerPort": 3000
          }
        ],
        "name": "ecsdemo-nodejs"
      }
    ],
    "memory": "512",
    "compatibilities": [
      "EC2",
      "FARGATE"
    ],
    "taskDefinitionArn": "<TASK_DEFINITION>",
    "family": "ecsdemo",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256"
  }